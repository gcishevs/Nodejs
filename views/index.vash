@html.extend('layout', function(model){ @html.block('body', function(model){

@if(model.error){
<p class="text-red">Error occured: @model.error</p>
}

<div class="row">
	<div class="list-group col-lg-6">
		@model.categories.forEach(function(category){
		<a class="list-group-item list-group-item-info" href="#"><span class="badge">@(category.notes ? category.notes.length : 0)</span>@category.name</a>
		})
	</div>

</div>


}) })